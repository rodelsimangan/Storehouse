@model dynamic
<style>
    p{
        text-align:left;
    }
</style>
<div class="container">
    <div class="divPanel">
        <!--Edit Main Content Area here-->
        <div id="divMain">
            <div class="row-fluid">
                <div class="span1"></div>
                <div class="span11">
                    <div class="breadcrumb">
                        <!--h5>Terms and Conditions</h5-->
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span1"></div>
                <div class="span10" id="divCMSContent">
                    <div class="row-fluid">
                        @foreach (var item in (IEnumerable<Storehouse.Model.TermsofUse>)ViewData["TermsofUseList"])
                        {
                            <div class="col-md-12" style="word-break:break-all; word-wrap:break-word; text-align:center;"><h4>@item.Title</h4></div>
                            <div class="col-md-12" style="word-break:break-all; word-wrap:break-word; text-align:center;"><span style="font-weight:bold">@item.Subtitle</span></div>
                            <br />
                            <div class="col-md-8 editor-label" style="padding-right:20px; text-align:left!important;">
                                 @Html.Raw(System.Web.HttpUtility.HtmlDecode(item.Markup))
                            </div>
                        }

                    </div>
                </div>
                <div class="span1"></div>
            </div>
            <!--End Main Content-->
        </div>
        <!--div id="footerInnerSeparator"></div-->
    </div>
</div>